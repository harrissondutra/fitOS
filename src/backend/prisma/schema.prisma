model MigrationJob {
  id               String              @id @default(cuid())
  organizationId   String              @map("organization_id")
  fromStrategy     DbStrategy          @map("from_strategy")
  toStrategy       DbStrategy          @map("to_strategy")
  fromConnectionId String?             @map("from_connection_id")
  toConnectionId   String?             @map("to_connection_id")
  status           MigrationStatus     @default(pending)
  progressPercent  Int                 @default(0) @map("progress_percent")
  currentStep      String?             @map("current_step")
  totalSteps       Int                 @default(0) @map("total_steps")
  backupId         String?             @map("backup_id")
  startedAt        DateTime?           @map("started_at")
  completedAt      DateTime?           @map("completed_at")
  errorLog         String?             @map("error_log")
  fromConnection   DatabaseConnection? @relation("FromConnection", fields: [fromConnectionId], references: [id])
  organization     Tenant              @relation(fields: [organizationId], references: [id], onDelete: Cascade)
  toConnection     DatabaseConnection? @relation("ToConnection", fields: [toConnectionId], references: [id])

  @@index([organizationId, startedAt])
  @@index([status])
  @@map("migration_jobs")
  @@schema("fitness")
}


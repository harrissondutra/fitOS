model DatabaseConnection {
  id                 String             @id @default(cuid())
  organizationId     String             @map("organization_id")
  name               String
  provider           DatabaseProvider
  host               String
  port               Int                @default(5432)
  databaseName       String             @map("database_name")
  username           String
  encryptedPassword  String             @map("encrypted_password")
  sshEnabled         Boolean            @default(false) @map("ssh_enabled")
  sshHost            String?            @map("ssh_host")
  sshPort            Int?               @default(22) @map("ssh_port")
  sshUsername        String?            @map("ssh_username")
  encryptedSshKey    String?            @map("encrypted_ssh_key")
  sslEnabled         Boolean            @default(false) @map("ssl_enabled")
  sslCert            String?            @map("ssl_cert")
  connectionPoolSize Int                @default(10) @map("connection_pool_size")
  status             ConnectionStatus   @default(active)
  healthCheckUrl     String?            @map("health_check_url")
  lastHealthCheckAt  DateTime?          @map("last_health_check_at")
  createdAt          DateTime           @default(now()) @map("created_at")
  updatedAt          DateTime           @updatedAt @map("updated_at")
  organization       Tenant             @relation(fields: [organizationId], references: [id], onDelete: Cascade)
  databaseInstances  DatabaseInstance[]
  databaseMetrics    DatabaseMetric[]
  migrationJobs      MigrationJob[]     @relation("FromConnection")
  migrationJobsDest  MigrationJob[]     @relation("ToConnection")

  @@index([organizationId])
  @@index([provider, status])
  @@map("database_connections")
  @@schema("fitness")
}

